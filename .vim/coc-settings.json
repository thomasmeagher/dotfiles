{
    "coc.preferences.jumpCommand": "drop",
    "coc.preferences.formatOnSaveFiletypes": [
        "css",
        "javascript",
        "javascriptreact",
        "json",
        "typescript",
        "typescriptreact"
    ],
    "diagnostic.errorSign": ">>",
    "diagnostic.warningSign": "!!",
    "diagnostic.infoSign": ">>",
    "diagnostic.hintSign": ">>",
    "diagnostic-languageserver.formatters": {
        "swiftlint": {
            "command": "swiftlint",
            "args": [
                "autocorrect"
            ]
        }
    },
    "diagnostic-languageserver.formatFiletypes": {
        "swift": "swiftlint"
    },
    "eslint.autoFixOnSave": true,
    "eslint.enable": true,
    "eslint.packageManager": "yarn",
    "snippets.extends": {
        "javascriptreact": [
            "javascript"
        ],
        "typescriptreact": [
            "javascript"
        ],
        "typescript": [
            "javascript"
        ]
    },
    "snippets.ultisnips.enable": true,
    "tailwindCSS.headwind.runOnSave": false
}
