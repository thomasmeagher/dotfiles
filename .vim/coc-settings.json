{
    "coc.preferences.formatOnSaveFiletypes": [
        "css",
        "javascript",
        "javascriptreact",
        "json",
        "typescript",
        "typescriptreact"
    ],
    "coc.preferences.hoverTarget": "float",
    "coc.preferences.jumpCommand": "split",
    "diagnostic.errorSign": ">>",
    "diagnostic.warningSign": "!!",
    "diagnostic.infoSign": ">>",
    "diagnostic.hintSign": ">>",
    "eslint.autoFixOnSave": true,
    "eslint.enable": true,
    "eslint.packageManager": "yarn",
    "snippets.extends": {
        "javascriptreact": [
            "javascript"
        ],
        "typescriptreact": [
            "javascript"
        ],
        "typescript": [
            "javascript"
        ]
    },
    "snippets.ultisnips.enable": true,
    "tailwindCSS.headwind.runOnSave": false
}
